      <ImageView fitHeight="119.0" fitWidth="226.0" layoutX="143.0" layoutY="190.0" pickOnBounds="true">
         <image>
            <Image url="@../icon/info.png" />
         </image>
         <effect>
            <ColorAdjust contrast="-0.1" />
         </effect>
      </ImageView>
