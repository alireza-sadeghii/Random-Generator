      </ImageView>
      <ImageView fitHeight="69.0" fitWidth="88.0" layoutX="48.0" layoutY="257.0" onMouseClicked="#openGitHub" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icon/github.png" />
         </image>
         <effect>
            <ColorAdjust contrast="-0.07" />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label layoutX="158.0" layoutY="128.0" onMouseClicked="#emailClick" prefHeight="17.0" prefWidth="199.0" text="alireza.sproject@gmail.com">
         <font>
            <Font name="Times New Roman Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Label>
      <ImageView fitHeight="243.0" fitWidth="199.0" layoutX="379.0" layoutY="106.0" pickOnBounds="true">
         <image>
            <Image url="@../icon/me.jpg" />
         </image>
