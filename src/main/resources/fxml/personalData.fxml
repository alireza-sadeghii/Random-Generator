                <Font name="Times New Roman Bold Italic" size="33.0" />
            </font>
        </Label>
        <ImageView fitHeight="64.0" fitWidth="60.0" layoutY="-3.0" onMouseClicked="#goHome" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../icon/home.jpg" />
            </image>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <effect>
                <ColorAdjust brightness="-0.05" />
            </effect></ImageView>
      <ImageView fitHeight="90.0" fitWidth="60.0" layoutX="60.0" layoutY="106.0" onMouseClicked="#emailClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icon/email.png" />
         </image>
         <effect>
            <ColorAdjust brightness="-0.02" contrast="-0.03" />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="60.0" layoutX="62.0" layoutY="186.0" onMouseClicked="#openLinkedIn" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icon/linkedin.jpg" />
