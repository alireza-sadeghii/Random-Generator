                  <Stop>
                     <color>
                        <Color red="0.8899999856948853" green="0.062300000339746475" blue="0.062300000339746475" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="0.9252253770828247" green="0.9736841917037964" blue="0.004507797304540873" />
                     </color>
                  </Stop>
               </stops>
            </LinearGradient>
